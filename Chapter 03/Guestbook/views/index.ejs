<% include header.ejs %>
<% if (entries.length > 0) { %>
<% entries.forEach(function (entry) { %>
<div class="row">
    <div class="panel panel-default">
        <div class="panel-heading">
            <%= entry.title %>
            <div class="text-muted pull-right"><%= entry.published %></div>
        </div>
        <div class="panel-body">
            <%= entry.body %>
        </div>
    </div>
</div>
<% }); %>
<% } else { %>
<h2> No Entries Found!</h2>
<% } %>
<% include footer.ejs %>
